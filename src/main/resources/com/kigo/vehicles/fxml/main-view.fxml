<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kigo.vehicles.controller.MainController">
    <Pane fx:id="root" />
        <ImageView fx:id="popup" fitHeight="208.0" fitWidth="346.0" layoutX="120.0" layoutY="89.0" visible="false">
        <Image url="@../images/noarraycap.jpg" />
    </ImageView>
    <VBox fx:id="labels" style="-fx-background-color: rgba(20,239,20,0.32); -fx-background-radius: 10; -fx-padding: 10px;">
        <Label fx:id="timeLabel" />
        <Label fx:id="vehiclesInfo">
            <font>
                <Font name="JetBrainsMono Nerd Font Mono" size="20" />
            </font>
            <textFill><Color blue="1"/></textFill>
        </Label>
    </VBox>
</AnchorPane>
